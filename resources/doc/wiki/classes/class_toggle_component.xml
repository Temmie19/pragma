<root>
	<collection type="library">
		<name>ents</name>
		<collection type="class">
			<name>ToggleComponent</name>
			<derivedFrom>ents.EntityComponent</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method" shared="1">
					<name>TurnOn</name>
					<desc>Turns the component on.</desc>
					<lua></lua>
					<overload></overload>
				</function>
				<function type="method" shared="1">
					<name>TurnOff</name>
					<desc>Turns the component off.</desc>
					<lua></lua>
					<overload></overload>
				</function>
				<function type="method" shared="1">
					<name>Toggle</name>
					<desc>Toggles the component.</desc>
					<lua></lua>
					<overload></overload>
				</function>
				<function type="method" shared="1">
					<name>IsTurnedOn</name>
					<desc>Returns true if the component is turned on, otherwise false.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="bool">turnedOn</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>SetTurnedOn</name>
					<desc>Turns the component on or off.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="bool">turnedOn</arg>
						</parameters>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetTurnedOnProperty</name>
					<desc>Returns the component's turned-on property.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="util.BoolProperty">turnedOn</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<enum_set>
				<name>Event</name>
				<enum>EVENT_ON_TURN_ON</enum>
				<enum>EVENT_ON_TURN_OFF</enum>
			</enum_set>
		</collection>
	</collection>
</root>
