<root>
	<collection type="library">
		<name>gui</name>
		<collection type="class" client="1" gui="1">
			<name>TextEntry</name>
			<derivedFrom>gui.Element</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method">
					<name>SetText</name>
					<desc>Changes the element's text.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">text</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetText</name>
					<desc>Returns the element's text.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="string">text</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<function type="method">
				<name>IsNumeric</name>
				<desc>Returns ''true'' if only numeric values are allowed.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="bool">isNumeric</arg>
					</returnValues>
				</overload>
			</function>
			<group>
				<function type="method">
					<name>IsEditable</name>
					<desc>Returns ''true'' if this text entry is editable.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="bool">isEditable</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetEditable</name>
					<desc>If set to ''false'', this text entry can't be edited.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="bool">editable</arg>
						</parameters>
					</overload>
				</function>
			</group>
			<group>
				<function type="method">
					<name>SetMaxLength</name>
					<desc>Changes the maximum allowed characters for this text entry.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">maxLength</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetMaxLength</name>
					<desc>Returns the maximum allowed characters for this text entry.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="int">maxLength</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<group>
				<function type="method">
					<name>IsMultiLine</name>
					<desc>Returns ''true'' if this is a multi-line text entry.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="bool">isMultiLine</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetMultiLine</name>
					<desc>If set to ''true'', the text entry can accept multiple lines.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="bool">multiLine</arg>
						</parameters>
					</overload>
				</function>
			</group>
		</collection>
	</collection>
</root>
