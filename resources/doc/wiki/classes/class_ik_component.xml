<root>
	<collection type="library">
		<name>ents</name>
		<collection type="class">
			<name>IKComponent</name>
			<derivedFrom>ents.EntityComponent</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method" shared="1">
					<name>SetIKControllerEnabled</name>
					<desc>Enables or disables the specified ik controller.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">ikControllerId</arg>
							<arg type="bool">enabled</arg>
						</parameters>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>IsIKControllerEnabled</name>
					<desc>Returns ''true'' if the specified ik controller is enabled.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">ikControllerId</arg>
						</parameters>
						<returnValues>
							<arg type="bool">enabled</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<group>
				<function type="method" shared="1">
					<name>SetIKEffectorPos</name>
					<desc>Changes the effector position for the specified ik controller effector.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">ikControllerId</arg>
							<arg type="int">effectorIdx</arg>
							<arg type="math.Vector">position</arg>
						</parameters>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetIKEffectorPos</name>
					<desc>Returns the effector position for the specified ik controller effector.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">ikControllerId</arg>
							<arg type="int">effectorIdx</arg>
						</parameters>
						<returnValues>
							<arg type="math.Vector">position</arg>
						</returnValues>
					</overload>
				</function>
			</group>
		</collection>
	</collection>
</root>
