<root>
	<collection type="library">
		<name>gui</name>
		<collection type="class" client="1" gui="1">
			<name>Text</name>
			<derivedFrom>gui.Element</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method">
					<name>SetText</name>
					<desc>Changes the element's text.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">text</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetText</name>
					<desc>Returns the element's text.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="string">text</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<function type="method">
				<name>SetFont</name>
				<desc>Changes the element's font. The font has to be initialied with [pragma:ref]engine.create_front[/pragma:ref] first!</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="string">font</arg>
					</parameters>
				</overload>
			</function>
			<group>
				<function type="method">
					<name>EnableShadow</name>
					<desc>Enables or disables the shadow for this element.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="bool">shadowEnabled</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>IsShadowEnabled</name>
					<desc>Returns ''true'' if the shadow is enabled for this element.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="bool">shadowEnabled</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetShadowColor</name>
					<desc>Changes the shadow color for this element.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="util.Color">shadowColor</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetShadowColor</name>
					<desc>Returns the shadow color for this element.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="util.Color">shadowColor</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetShadowOffset</name>
					<desc>Changes the offset of the shadow relative to the element.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="math.Vector2">shadowOffset</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetShadowOffset</name>
					<desc>Returns the offset of the shadow relative to the element.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="math.Vector2">shadowOffset</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetShadowXOffset</name>
					<desc>Changes the x-offset of the shadow relative to the element.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="float">xOffset</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetShadowXOffset</name>
					<desc>Returns the x-offset of the shadow relative to the element.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">xOffset</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetShadowYOffset</name>
					<desc>Changes the y-offset of the shadow relative to the element.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="float">yOffset</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetShadowYOffset</name>
					<desc>Returns the y-offset of the shadow relative to the element.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">yOffset</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetShadowAlpha</name>
					<desc>Changes the transparency of the shadow.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="float">shadowAlpha</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetShadowAlpha</name>
					<desc>Returns the transparency of the shadow.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">shadowAlpha</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>SetShadowBlurSize</name>
					<desc></desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="float">blurSize</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetShadowBlurSize</name>
					<desc></desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">blurSize</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<function type="method">
				<name>GetTextProperty</name>
				<desc>Returns the text property.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="util.StringProperty">textProperty</arg>
					</returnValues>
				</overload>
			</function>
		</collection>
	</collection>
</root>
