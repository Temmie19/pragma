<root>
	<collection type="library">
		<name>ents</name>
		<collection type="class">
			<name>OwnableComponent</name>
			<derivedFrom>ents.EntityComponent</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method" shared="1">
					<name>GetOwner</name>
					<desc>Returns the weapon's owner.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="ents.Entity">owner</arg>
						</returnValues>
