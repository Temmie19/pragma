						<parameters>
							<arg type="ents.Entity" default="NULL">owner</arg>
						</parameters>
					</overload>
				</function>
			</group>
			<enum_set>
				<name>Event</name>
				<enum>EVENT_ON_OWNER_CHANGED</enum>
			</enum_set>
		</collection>
	</collection>
