		<collection type="class">
			<name>TimeScaleComponent</name>
			<derivedFrom>ents.EntityComponent</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method" shared="1">
					<name>GetTimeScale</name>
					<desc>Returns the entity's time scale.</desc>
					<lua></lua>
