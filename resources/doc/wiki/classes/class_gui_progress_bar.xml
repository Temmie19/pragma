<root>
	<collection type="library">
		<name>gui</name>
		<collection type="class" client="1" gui="1">
			<name>ProgressBar</name>
			<derivedFrom>gui.Element</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method">
					<name>SetProgress</name>
					<desc>Changes the progress of this element, with 0 = 0% and 1 = 100%.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="float">progress</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetProgress</name>
					<desc>Returns the progress of this element, with 0 = 0% and 1 = 100%.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">progress</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<group>
				<function type="method">
					<name>SetValue</name>
					<desc>Changes the element's value.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="float">value</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetValue</name>
					<desc>Returns the element's value.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">progress</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<function type="method">
				<name>SetRange</name>
				<desc>Changes the range of the element.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="float">min</arg>
						<arg type="float">max</arg>
						<arg type="float" default="1.0">stepSize</arg>
					</parameters>
				</overload>
			</function>
			<group>
				<function type="method">
					<name>SetOptions</name>
					<desc>Takes a table of strings and assigns them as options for this progress bar.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="table" subType="string">options</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>AddOption</name>
					<desc>Adds a new option for this progress bar.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">option</arg>
						</parameters>
					</overload>
				</function>
			</group>
			<function type="method">
				<name>SetPostFix</name>
				<desc>Sets the postfix for the progress bar, which will be appended to the current value.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="string">postFix</arg>
					</parameters>
				</overload>
			</function>
		</collection>
	</collection>
</root>
