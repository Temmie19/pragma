<root>
	<collection type="library">
		<name>ents</name>
		<collection type="class">
			<name>IOComponent</name>
			<derivedFrom>ents.EntityComponent</derivedFrom>
			<desc>TODO</desc>
			<function type="method" shared="1">
				<name>StoreOutput</name>
				<desc>Stores the specified output for the next time(s) the output is fired by the entity.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="string">outputName</arg>
						<arg type="string">entityFilter</arg>
						<arg type="string">input</arg>
						<arg type="string">inputParameters</arg>
						<arg type="float" default="0.0">fireDelay</arg>
						<arg type="int" default="-1">times</arg>
					</parameters>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>FireOutput</name>
				<desc>Fires the specified output for the entity.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="string">outputName</arg>
						<arg type="ents.Entity">activator</arg>
					</parameters>
				</overload>
			</function>
			<enum_set>
				<name>Event</name>
				<enum>EVENT_HANDLE_INPUT</enum>
			</enum_set>
		</collection>
	</collection>
</root>
