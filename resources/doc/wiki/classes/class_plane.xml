<root>
	<collection type="library">
		<name>math</name>
		<collection type="class">
			<name>Plane</name>
			<desc>TODO</desc>
			<function type="method" shared="1">
				<name>Copy</name>
				<desc>Returns a copy of this plane.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="math.Plane">plane</arg>
					</returnValues>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>GetNormal</name>
				<desc>Returns the normal of this plane.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="math.Vector">normal</arg>
					</returnValues>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>GetPos</name>
				<desc>Returns a position on the plane.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="math.Vector">position</arg>
					</returnValues>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>MoveToPos</name>
				<desc>Moves the plane to the specified position without changing its normal.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="math.Vector">position</arg>
					</parameters>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>Rotate</name>
				<desc>Rotates the plane by the specified angles.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="math.EulerAngles">angles</arg>
					</parameters>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>GetCenterPos</name>
				<desc>Returns the plane's center position.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="math.Vector">position</arg>
					</returnValues>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>Transform</name>
				<desc>Transforms the plane by the specified matrix.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="math.Mat4">transformMatrix</arg>
					</parameters>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>constructor</name>
				<desc>Creates a new Plane object.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="math.Vector">a</arg>
						<arg type="math.Vector">b</arg>
						<arg type="math.Vector">c</arg>
					</parameters>
					<returnValues>
						<arg type="math.Plane">plane</arg>
					</returnValues>
				</overload>
				<overload>
					<parameters>
						<arg type="math.Vector">normal</arg>
						<arg type="math.Vector">position</arg>
					</parameters>
					<returnValues>
						<arg type="math.Plane">plane</arg>
					</returnValues>
				</overload>
				<overload>
					<parameters>
						<arg type="math.Vector">normal</arg>
						<arg type="float">distance</arg>
					</parameters>
					<returnValues>
						<arg type="math.Plane">plane</arg>
					</returnValues>
				</overload>
			</function>
		</collection>
	</collection>
</root>
