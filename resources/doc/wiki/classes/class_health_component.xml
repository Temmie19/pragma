<root>
	<collection type="library">
		<name>ents</name>
		<collection type="class">
			<name>HealthComponent</name>
			<derivedFrom>ents.EntityComponent</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method" shared="1">
					<name>GetHealth</name>
					<desc>Returns the entity's current health.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="int">health</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>SetHealth</name>
					<desc>Changes the entity's current health.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">health</arg>
						</parameters>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetHealthProperty</name>
					<desc>Returns the component's health property.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="util.IntProperty">healthProperty</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<group>
				<function type="method" shared="1">
					<name>GetMaxHealth</name>
					<desc>Returns the entity's maximum health.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="int">maxHealth</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>SetMaxHealth</name>
					<desc>Changes the entity's maximum health.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">maxHealth</arg>
						</parameters>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetMaxHealthProperty</name>
					<desc>Returns the component's max health property.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="util.IntProperty">maxHealthProperty</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<enum_set>
				<name>Event</name>
				<enum>EVENT_ON_TAKEN_DAMAGE</enum>
				<enum>EVENT_ON_HEALTH_CHANGED</enum>
			</enum_set>
		</collection>
	</collection>
</root>
