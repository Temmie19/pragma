<root>
	<collection type="library">
		<name>phys</name>
		<collection type="class">
			<name>Heightfield</name>
			<derivedFrom>phys.Shape</derivedFrom>
			<desc>TODO</desc>
			<group>
				<function type="method" shared="1">
					<name>GetHeight</name>
					<desc>Returns the heightfield's height for the specified coordinates.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">x</arg>
							<arg type="int">y</arg>
						</parameters>
						<returnValues>
							<arg type="float">height</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>SetHeight</name>
					<desc>Changes the heightfield's height for the specified coordinates.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">x</arg>
							<arg type="int">y</arg>
							<arg type="float">height</arg>
						</parameters>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetWidth</name>
					<desc>Changes the heightfield's width.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">width</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetLength</name>
					<desc>Changes the heightfield's length.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">length</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetMaxHeight</name>
					<desc>Changes the heightfield's max width.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="float">maxHeight</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<function type="method" shared="1">
				<name>GetUpAxis</name>
				<desc>Changes the heightfield's up-axis.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="int">axisId</arg>
					</returnValues>
				</overload>
			</function>
		</collection>
	</collection>
</root>
