					<lua></lua>
					<overload>
						<returnValues>
							<arg type="bool">isConvex</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>IsConvexHull</name>
					<desc>Returns ''true'' if the shape is a convex hull shape.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="bool">isConvex</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>IsHeightfield</name>
					<desc>Returns ''true'' if the shape is a height field.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="bool">isConvex</arg>
						</returnValues>
