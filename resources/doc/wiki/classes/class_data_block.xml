<root>
	<collection type="library">
		<name>util</name>
		<collection type="class" gui="1">
			<name>DataBlock</name>
			<desc>A data-block contains keyvalue-pairs of various different types.</desc>
			<group>
				<function type="method" shared="1">
					<name>GetInt</name>
					<desc>Returns the value associated with the specified key as integer.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">key</arg>
						</parameters>
						<returnValues>
							<arg type="int">value</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetFloat</name>
					<desc>Returns the value associated with the specified key as float.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">key</arg>
						</parameters>
						<returnValues>
							<arg type="float">value</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetBool</name>
					<desc>Returns the value associated with the specified key as boolean.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">key</arg>
						</parameters>
						<returnValues>
							<arg type="bool">value</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method" shared="1">
					<name>GetString</name>
					<desc>Returns the value associated with the specified key as string.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">key</arg>
						</parameters>
						<returnValues>
							<arg type="string">value</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<function type="method" shared="1">
				<name>GetData</name>
				<desc>Returns the child associated with the specified key, or nil if there is none.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="string">key</arg>
					</parameters>
					<returnValues>
						<arg type="util.DataBlock">value</arg>
					</returnValues>
				</overload>
			</function>
			<function type="method" shared="1">
				<name>SetValue</name>
				<desc>Specifies the value associated with the given key.</desc>
				<lua></lua>
				<overload>
					<parameters>
						<arg type="string">type</arg>
						<arg type="string">key</arg>
						<arg type="string">value</arg>
					</parameters>
				</overload>
			</function>
		</collection>
	</collection>
</root>
