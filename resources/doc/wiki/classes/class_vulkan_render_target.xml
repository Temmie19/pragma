<root>
	<collection type="library">
		<name>vulkan</name>
		<collection type="class" client="1" gui="1">
			<name>RenderTarget</name>
			<desc>TODO</desc>
			<function type="method">
				<name>tostring</name>
				<desc>Converts the render target to a string.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="string">renderTarget</arg>
					</returnValues>
				</overload>
			</function>
			<group>
				<function type="method">
					<name>SetDebugName</name>
					<desc>Changes the descriptor set's debug name.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="string">debugName</arg>
						</parameters>
					</overload>
				</function>
				<function type="method">
					<name>GetDebugName</name>
					<desc>Returns the descriptor set's debug name.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="string">debugName</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<group>
				<function type="method">
					<name>GetTexture</name>
					<desc>Returns the render target's texture.</desc>
					<lua></lua>
					<overload>
						<parameters>
							<arg type="int">textureIndex</arg>
						</parameters>
						<returnValues>
							<arg type="vulkan.Texture">texture</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>GetRenderPass</name>
					<desc>Returns the render target's render pass.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="vulkan.RenderPass">renderPass</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>GetFramebuffer</name>
					<desc>Returns the render target's framebuffer.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="vulkan.Framebuffer">framebuffer</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<group>
				<function type="method">
					<name>GetWidth</name>
					<desc>Returns the width of the render target's image.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="int">width</arg>
						</returnValues>
					</overload>
				</function>
				<function type="method">
					<name>GetHeight</name>
					<desc>Returns the height of the render target's image.</desc>
					<lua></lua>
					<overload>
						<returnValues>
							<arg type="int">height</arg>
						</returnValues>
					</overload>
				</function>
			</group>
			<function type="method">
				<name>GetFormat</name>
				<desc>Returns the format of the render target's image.</desc>
				<lua></lua>
				<overload>
					<returnValues>
						<arg type="vulkan.Format">format</arg>
					</returnValues>
				</overload>
			</function>
		</collection>
	</collection>
</root>
